{
    "compilerOptions": {
        "target": "es2019",
        "module": "es2015",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "sourceMap": true,
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
            "src/*": [
                "src/*"
            ]
        },
        "strict": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true
    }
}
